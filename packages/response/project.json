{
  "name": "response",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/response/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "pnpm build",
            "forwardAllArgs": true
          }
        ],
        "cwd": "packages/response"
      },
      "outputs": [
        "{workspaceRoot}/packages/response/dist"
      ]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx tools/scripts/publish.ts response ",
        "outputPath": "packages/response/dist"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/response/**/*.ts"
        ]
      }
    },
    "test2": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/packages/response"
      ],
      "options": {
        "jestConfig": "packages/response/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/packages/response"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/response"
      }
    },
    "vitest": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "pnpm test",
            "forwardAllArgs": true
          }
        ],
        "cwd": "packages/response"
      }
    }
  },
  "tags": []
}
